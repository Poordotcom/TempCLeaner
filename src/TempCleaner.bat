@echo off
title Cleaning Temporary Files
echo Cleaning Temporary Files...
echo ==========================
echo.

:: Define temporary folders to clean
set "temp_folders=C:\Windows\Temp %USERPROFILE%\AppData\Local\Temp C:\Windows\Prefetch %USERPROFILE%\Local Settings\Temp %USERPROFILE%\AppData\Local\Temp"

:: Loop through each temporary folder
for %%F in (%temp_folders%) do (
    echo Cleaning temporary files in %%F ...
    cd /d %%F
    del /s /q *.*
)
echo.

